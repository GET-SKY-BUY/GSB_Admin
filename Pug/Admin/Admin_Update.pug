                <select  id="Assistant_Type" class="InputText">
                    <option value="#{Assistant_Type}" selected>#{Assistant_Type}</option>
                    <option value="Seller Assistant">Seller Assistant</option>
                    <option value="Product Assistant">Product Assistant</option>
                    <option value="Order Assistant">Order Assistant</option>
                    <option value="Delivery Assistant">Delivery Assistant</option>
                    <option value="Contact us Assistant">Contact us Assistant</option>
                    <option value="Payment Assistant">Payment Assistant</option>
                    <option value="Complaint Assistant">Complaint Assistant</option>
